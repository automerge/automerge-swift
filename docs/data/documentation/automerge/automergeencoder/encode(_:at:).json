            {
              "code" : "path",
              "type" : "codeVoice"
            },
            {
              "text" : " parameter accepts any type conforming to the ",
              "type" : "text"
            },
            {
              "code" : "CodingKey",
              "type" : "codeVoice"
            },
            {
              "text" : " protocol.",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "This library provides a type-erased coding key, ",
              "type" : "text"
            },
            {
