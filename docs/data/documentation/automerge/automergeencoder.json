            {
              "code" : "AutomergeEncoder",
              "type" : "codeVoice"
            },
            {
              "text" : " to encode your data into an Automerge document.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "The following example illustrates encoding ",
              "type" : "text"
            },
            {
              "code" : "ColorList",
              "type" : "codeVoice"
            },
            {
              "text" : ", a type that conforms to ",
              "type" : "text"
            },
            {
              "code" : "Codable",
              "type" : "codeVoice"
            },
            {
              "text" : ", into an Automerge document:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "struct ColorList: Codable {",
            "    var colors: [String]",
            "}",
            "",
            "let doc = Document()",
            "let encoder = AutomergeEncoder(doc: doc)",
            "",
            "var myColors = ColorList(colors: [\"blue\", \"red\"])",
            "try encoder.encode(myColors)"
          ],
          "syntax" : "swift",
